{
  "name": "@erickva/auth-kit-cli",
  "version": "0.1.0",
  "description": "CLI tool for scaffolding Auth Kit projects",
  "bin": {
    "auth-kit": "./dist/index.js",
    "create-auth-app": "./dist/index.js"
  },
  "files": [
